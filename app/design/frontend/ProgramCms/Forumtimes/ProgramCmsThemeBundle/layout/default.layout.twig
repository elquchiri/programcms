{% page %}
    {% move name="header.panel.wrapper" destination="footer.legal.copyright" before="copyright" %}

    {% referenceContainer name="navbar.collapse" remove="true" %}{% endReferenceContainer %}

    {% referenceContainer name="logo.container" %}
        {% block name="account.resume" class="ProgramCms\\CoreBundle\\View\\Element\\Template" template="@ProgramCmsThemeBundle/account.html.twig" after="logo" %}{% endBlock %}
    {% endReferenceContainer %}

    {% referenceContainer name="navbar" %}
        {% container name="forum.navbar" htmlTag="div" htmlClass="collapse navbar-collapse justify-content-end" %}
            {% block name="forum.home" class="ProgramCms\\CoreBundle\\View\\Element\\Template" template="@ProgramCmsThemeBundle/forum_home.html.twig" %}{% endBlock %}
        {% endContainer %}
    {% endReferenceContainer %}
{% endPage %}