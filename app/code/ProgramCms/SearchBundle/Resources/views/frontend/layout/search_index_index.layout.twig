{% page %}
    {% referenceContainer name="content" %}
        {% container name="search.row" htmlTag="div" htmlClass="row" %}
            {% container name="search.row.filters" htmlTag="div" htmlClass="col-md-4" %}
                {% block name="search.filters" class="ProgramCms\\SearchBundle\\Block\\Filters" template="@ProgramCmsSearchBundle/filters.html.twig" %}{% endBlock %}
            {% endContainer %}
            {% container name="search.row.results" htmlTag="div" htmlClass="col-md-8" %}
                {% block name="search.results" class="ProgramCms\\SearchBundle\\Block\\Results" template="@ProgramCmsSearchBundle/results.html.twig" %}{% endBlock %}
            {% endContainer %}
        {% endContainer %}
    {% endReferenceContainer %}
{% endPage %}