{% EFPage layout="admin-2columns-left" %}
    {% EFCss ['bundles/programcmsconfig/css/menu.css', 'bundles/programcmsui/css/components/ef_collapse.css'] %}
    {% EFJs ['bundles/programcmsconfig/js/config_menu.js'] %}
    {% EFTitle %}Edit Configuration{% endEFTitle %}

    {% EFReferenceContainer name="buttons.bar" %}
        {% EFBlock name="website.switcher" class="ProgramCms\\UiBundle\\Block\\Toolbar\\WebsiteSwitcher" template="@ProgramCmsUiBundle/toolbar/website_switcher.html.twig" %}{% endEFBlock %}
        {% EFBlock name="toolbar.actions" class="ProgramCms\\ConfigBundle\\Block\\ToolbarActions" template="@ProgramCmsUiBundle/toolbar/toolbar_buttons.html.twig" %}{% endEFBlock %}
    {% endEFReferenceContainer %}

    {% EFReferenceContainer name="sidebar.main" %}
        {% EFBlock name="config.menu.block" class="ProgramCms\\ConfigBundle\\Block\\Configuration" template="@ProgramCmsConfigBundle/config_menu.html.twig" %}{% endEFBlock %}
    {% endEFReferenceContainer %}

    {% EFReferenceContainer name="content" %}
        {% EFBlock name="config.block" class="ProgramCms\\ConfigBundle\\Block\\Configuration" template="@ProgramCmsConfigBundle/config.html.twig" %}{% endEFBlock %}
    {% endEFReferenceContainer %}
{% endEFPage %}