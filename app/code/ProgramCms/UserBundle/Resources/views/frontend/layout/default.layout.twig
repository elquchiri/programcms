{% page %}
    {% referenceContainer name="navbar.collapse" %}
        {% block name="account.dropdown" class="ProgramCms\\UserBundle\\Block\\Account\\AccountDropDown" template="@ProgramCmsUserBundle/account_dropdown.html.twig" after="-" %}{% endBlock %}
    {% endReferenceContainer %}

    {% referenceBlock name="navigation.block" %}
        {% block name="navigation.community.link" class="ProgramCms\\ThemeBundle\\Block\\Html\\Item" %}
            {% arguments %}
                {% argument name="label" type="string" %}Community{% endArgument %}
                {% argument name="path" type="string" %}user_profile_index{% endArgument %}
                {% argument name="sortOrder" type="string" %}4{% endArgument %}
            {% endArguments %}
        {% endBlock %}
        {% block name="navigation.new_account.link" class="ProgramCms\\ThemeBundle\\Block\\Html\\Item" %}
            {% arguments %}
                {% argument name="label" type="string" %}Create an Account{% endArgument %}
                {% argument name="path" type="string" %}user_account_register{% endArgument %}
                {% argument name="sortOrder" type="string" %}8{% endArgument %}
                {% argument name="isAuth" type="boolean" %}false{% endArgument %}
            {% endArguments %}
        {% endBlock %}
        {% block name="navigation.login.link" class="ProgramCms\\ThemeBundle\\Block\\Html\\Item" %}
            {% arguments %}
                {% argument name="label" type="string" %}Sign In{% endArgument %}
                {% argument name="path" type="string" %}user_account_login{% endArgument %}
                {% argument name="sortOrder" type="string" %}9{% endArgument %}
                {% argument name="isAuth" type="boolean" %}false{% endArgument %}
            {% endArguments %}
        {% endBlock %}
    {% endReferenceBlock %}
{% endPage %}