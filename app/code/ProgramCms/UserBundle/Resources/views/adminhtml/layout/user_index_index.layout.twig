{% page %}
    {% referenceContainer name="content" %}
        {% block name="users_listing" class="ui.block.listing" %}
            {% arguments %}
                {% argument name="buttons" type="array" %}
                    {% argument name="new" type="string" %}ProgramCms\UserBundle\Model\Provider\Button\NewUserButton{% endArgument %}
                {% endArgument %}
                {% argument name="dataSource" type="array" %}
                    {% argument name="dataProvider" type="array" %}
                        {% argument name="class" type="string" %}ProgramCms\UserBundle\Model\Provider\Listing\DataProvider{% endArgument %}
                        {% argument name="primaryFieldName" type="string" %}entity_id{% endArgument %}
                        {% argument name="requestFieldName" type="string" %}id{% endArgument %}
                    {% endArgument %}
                {% endArgument %}
            {% endArguments %}
            {% block name="users_listing_columns" class="ui.block.columns" %}
                {% block name="selectionsColumn" class="ui.block.selectionsColumn" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="source" type="string" %}users_listing{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="entity_id" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}#{% endArgument %}
                            {% argument name="source" type="string" %}users_listing{% endArgument %}
                            {% argument name="filter" type="string" %}text{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="username" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}Username{% endArgument %}
                            {% argument name="source" type="string" %}users_listing{% endArgument %}
                            {% argument name="filter" type="string" %}text{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="name_prefix" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}Prefix{% endArgument %}
                            {% argument name="source" type="string" %}users_listing{% endArgument %}
                            {% argument name="filter" type="string" %}text{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="user_firstname" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}First Name{% endArgument %}
                            {% argument name="source" type="string" %}users_listing{% endArgument %}
                            {% argument name="filter" type="string" %}text{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="user_lastname" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}Last Name{% endArgument %}
                            {% argument name="source" type="string" %}users_listing{% endArgument %}
                            {% argument name="filter" type="string" %}text{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="email" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}Email{% endArgument %}
                            {% argument name="source" type="string" %}users_listing{% endArgument %}
                            {% argument name="filter" type="string" %}text{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="websiteView" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}Account Created in{% endArgument %}
                            {% argument name="source" type="string" %}users_listing{% endArgument %}
                            {% argument name="filter" type="string" %}select{% endArgument %}
                            {% argument name="filterOptions" type="string" %}ProgramCms\WebsiteBundle\Model\Provider\DataSource\WebsiteTree{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="confirmed_email" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}Confirmed email{% endArgument %}
                            {% argument name="source" type="string" %}users_listing{% endArgument %}
                            {% argument name="filter" type="string" %}select{% endArgument %}
                            {% argument name="filterOptions" type="string" %}ProgramCms\UiBundle\Model\Provider\DataSource\YesNo{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="birthday" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}Birthday{% endArgument %}
                            {% argument name="source" type="string" %}users_listing{% endArgument %}
                            {% argument name="filter" type="string" %}text{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="actionsColumn" class="ProgramCms\\UserBundle\\Ui\\Listing\\Column\\UserActions" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}Actions{% endArgument %}
                            {% argument name="source" type="string" %}users_listing{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
            {% endBlock %}
        {% endBlock %}
    {% endReferenceContainer %}
{% endPage %}