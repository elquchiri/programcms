{% page %}
    {% referenceBlock name="user_form" %}
        {% block name="user_addresses" class="ui.block.form.fieldset" %}
            {% arguments %}
                {% argument name="data" type="array" %}
                    {% argument name="label" type="string" %}Addresses{% endArgument %}
                {% endArgument %}
            {% endArguments %}
            {% block name="new_address" class="ProgramCms\\UserBundle\\Ui\\Component\\NewAddressButton" %}
                {% arguments %}
                    {% argument name="disabled" type="string" %}false{% endArgument %}
                    {% argument name="value" type="string" %}Add New Address{% endArgument %}
                    {% argument name="css_class" type="string" %}btn btn-sm btn-outline-primary float-end mb-3{% endArgument %}
                {% endArguments %}
            {% endBlock %}
            {% block name="user_addresses_listing" class="ui.block.listing" %}
                {% arguments %}
                    {% argument name="dataSource" type="array" %}
                        {% argument name="dataProvider" type="array" %}
                            {% argument name="class" type="string" %}ProgramCms\UserBundle\Model\Provider\Listing\UserAddress\DataProvider{% endArgument %}
                            {% argument name="foreignFieldName" type="string" %}user{% endArgument %}
                            {% argument name="requestFieldName" type="string" %}id{% endArgument %}
                        {% endArgument %}
                    {% endArgument %}
                    {% argument name="toolbar" type="array" %}
                        {% argument name="filters" type="boolean" %}true{% endArgument %}
                        {% argument name="search" type="boolean" %}true{% endArgument %}
                        {% argument name="pagination" type="boolean" %}true{% endArgument %}
                        {% argument name="massActions" type="array" %}
                            {% argument name="settings" type="array" %}
                                {% argument name="delete" type="array" %}
                                    {% argument name="url" type="string" %}delete{% endArgument %}
                                    {% argument name="label" type="string" %}Delete All{% endArgument %}
                                {% endArgument %}
                            {% endArgument %}
                        {% endArgument %}
                    {% endArgument %}
                {% endArguments %}
                {% block name="user_addresses_listing_columns" class="ui.block.columns" %}
                    {% block name="user_addresses_listing_selectionsColumn" class="ui.block.selectionsColumn" %}
                        {% arguments %}
                            {% argument name="data" type="array" %}
                                {% argument name="source" type="string" %}user_addresses_listing{% endArgument %}
                            {% endArgument %}
                        {% endArguments %}
                    {% endBlock %}
                    {% block name="city" class="ui.block.column" %}
                        {% arguments %}
                            {% argument name="data" type="array" %}
                                {% argument name="label" type="string" %}City{% endArgument %}
                                {% argument name="source" type="string" %}user_addresses_listing{% endArgument %}
                                {% argument name="type" type="string" %}text{% endArgument %}
                                {% argument name="filter" type="string" %}text{% endArgument %}
                            {% endArgument %}
                        {% endArguments %}
                    {% endBlock %}
                    {% block name="country_code" class="ui.block.column" %}
                        {% arguments %}
                            {% argument name="data" type="array" %}
                                {% argument name="label" type="string" %}Country{% endArgument %}
                                {% argument name="source" type="string" %}user_addresses_listing{% endArgument %}
                                {% argument name="type" type="string" %}text{% endArgument %}
                                {% argument name="filter" type="string" %}text{% endArgument %}
                            {% endArgument %}
                        {% endArguments %}
                    {% endBlock %}
                    {% block name="firstname" class="ui.block.column" %}
                        {% arguments %}
                            {% argument name="data" type="array" %}
                                {% argument name="label" type="string" %}First Name{% endArgument %}
                                {% argument name="source" type="string" %}user_addresses_listing{% endArgument %}
                                {% argument name="type" type="string" %}text{% endArgument %}
                                {% argument name="filter" type="string" %}text{% endArgument %}
                            {% endArgument %}
                        {% endArguments %}
                    {% endBlock %}
                    {% block name="lastname" class="ui.block.column" %}
                        {% arguments %}
                            {% argument name="data" type="array" %}
                                {% argument name="label" type="string" %}Last Name{% endArgument %}
                                {% argument name="source" type="string" %}user_addresses_listing{% endArgument %}
                                {% argument name="type" type="string" %}text{% endArgument %}
                                {% argument name="filter" type="string" %}text{% endArgument %}
                            {% endArgument %}
                        {% endArguments %}
                    {% endBlock %}
                    {% block name="postcode" class="ui.block.column" %}
                        {% arguments %}
                            {% argument name="data" type="array" %}
                                {% argument name="label" type="string" %}Postcode{% endArgument %}
                                {% argument name="source" type="string" %}user_addresses_listing{% endArgument %}
                                {% argument name="type" type="string" %}text{% endArgument %}
                                {% argument name="filter" type="string" %}text{% endArgument %}
                            {% endArgument %}
                        {% endArguments %}
                    {% endBlock %}
                    {% block name="street" class="ui.block.column" %}
                        {% arguments %}
                            {% argument name="data" type="array" %}
                                {% argument name="label" type="string" %}Street{% endArgument %}
                                {% argument name="source" type="string" %}user_addresses_listing{% endArgument %}
                                {% argument name="type" type="string" %}text{% endArgument %}
                                {% argument name="filter" type="string" %}text{% endArgument %}
                            {% endArgument %}
                        {% endArguments %}
                    {% endBlock %}
                    {% block name="actionsColumn" class="ProgramCms\\UserBundle\\Ui\\Listing\\Column\\UserAddressActions" %}
                        {% arguments %}
                            {% argument name="data" type="array" %}
                                {% argument name="label" type="string" %}Actions{% endArgument %}
                                {% argument name="source" type="string" %}user_addresses_listing{% endArgument %}
                            {% endArgument %}
                        {% endArguments %}
                    {% endBlock %}
                {% endBlock %}
            {% endBlock %}
        {% endBlock %}
    {% endReferenceBlock %}
{% endPage %}