{% page %}
    {% referenceContainer name="content" %}
        {% block name="url_rewrite_listing" class="ui.block.listing" %}
            {% arguments %}
                {% argument name="buttons" type="array" %}
                    {% argument name="new" type="string" %}ProgramCms\RewriteBundle\Model\Provider\Button\Url\NewButton{% endArgument %}
                {% endArgument %}
                {% argument name="dataSource" type="array" %}
                    {% argument name="dataProvider" type="array" %}
                        {% argument name="class" type="string" %}ProgramCms\RewriteBundle\Model\Provider\Listing\Url\DataProvider{% endArgument %}
                        {% argument name="primaryFieldName" type="string" %}url_rewrite_id{% endArgument %}
                    {% endArgument %}
                {% endArgument %}
            {% endArguments %}
            {% block name="toolbar" class="ui.block.listing.toolbar" %}
                {% block name="filters" class="ui.block.listing.filters" %}{% endBlock %}
                {% block name="search" class="ui.block.listing.search" %}{% endBlock %}
                {% block name="massActions" class="ui.block.listing.massActions" %}
                    {% arguments %}
                        {% argument name="settings" type="array" %}
                            {% argument name="delete" type="array" %}
                                {% argument name="url" type="string" %}delete{% endArgument %}
                                {% argument name="label" type="string" %}Delete All{% endArgument %}
                            {% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="pagination" class="ui.block.listing.pagination" %}{% endBlock %}
            {% endBlock %}
            {% block name="url_rewrite_listing_columns" class="ui.block.columns" %}
                {% block name="selectionsColumn" class="ui.block.selectionsColumn" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="source" type="string" %}url_rewrite_listing{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="url_rewrite_id" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}#{% endArgument %}
                            {% argument name="source" type="string" %}url_rewrite_listing{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="request_path" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}Request Path{% endArgument %}
                            {% argument name="source" type="string" %}url_rewrite_listing{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="target_path" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}Target Path{% endArgument %}
                            {% argument name="source" type="string" %}url_rewrite_listing{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="arguments" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}Arguments{% endArgument %}
                            {% argument name="source" type="string" %}url_rewrite_listing{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="redirect_type" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}Redirect Type{% endArgument %}
                            {% argument name="source" type="string" %}url_rewrite_listing{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="actionsColumn" class="ProgramCms\\RewriteBundle\\Ui\\Grid\\Column\\Url\\Actions" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}Actions{% endArgument %}
                            {% argument name="source" type="string" %}url_rewrite_listing{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
            {% endBlock %}
        {% endBlock %}
    {% endReferenceContainer %}
{% endPage %}