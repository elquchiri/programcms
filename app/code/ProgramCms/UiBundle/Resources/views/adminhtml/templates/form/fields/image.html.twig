<img
    id="{{ block.getId() }}"
    alt="{{ block.getLabel() }}"
    src="{{ asset(block.getSrc()) }}"
    style="width: 100%; height: 300px; margin-top: 11px;"
    aria-describedby="{{ block.getId() }}" />