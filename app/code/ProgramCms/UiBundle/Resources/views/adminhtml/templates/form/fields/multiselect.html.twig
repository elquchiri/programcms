<select
    id="{{ block.getId() }}"
    name="{{ block.getName() }}"
    class="form-select multiselect"
    multiple
    {% if block.isDisabled() %}disabled="disabled" {% endif %}
>
    {{ block.getOptions()|raw }}
</select>
