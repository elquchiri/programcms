{% layout %}
    {% require handle="admin-empty" %}
    {% referenceContainer name="page.wrapper" %}
        {% container name="page.menu" %}{% endContainer %}
    {% endReferenceContainer %}
    {% referenceContainer name="page.content" %}
        {% container name="main.wrapper" htmlTag="div" htmlClass="main-wrapper" %}
            {% container name="notices.wrapper" before="-" %}
                {% container name="notifications" htmlTag="div" htmlClass="notices-notifications" %}{% endContainer %}
                {% container name="notification.window" before="-" %}{% endContainer %}
            {% endContainer %}
            {% container name="header.container" htmlTag="header" htmlClass="page-header" before="main.content" %}{% endContainer %}
            {% container name="main.top" %}{% endContainer %}
            {% container name="page.main.actions" htmlTag="nav" htmlClass="navbar navbar-expand-lg page-main-actions" %}{% endContainer %}
            {% container name="messages.wrapper" htmlTag="div" htmlClass="messages-wrapper" %}
                {% container name="page.messages" %}{% endContainer %}
            {% endContainer %}
            {% container name="page.main.container" htmlTag="div" htmlClass="row me-0 content-container" %}
                {% container name="main.col" htmlTag="div" htmlClass="col main-col" %}
                    {% container name="content" %}{% endContainer %}
                {% endContainer %}
            {% endContainer %}
            {% container name="footer.container" htmlTag="footer" htmlClass="footer" %}{% endContainer %}
        {% endContainer %}
    {% endReferenceContainer %}
{% endLayout %}