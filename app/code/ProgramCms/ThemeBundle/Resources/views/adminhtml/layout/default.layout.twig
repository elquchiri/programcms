{% page layout="admin-1column" %}
    {% referenceContainer name="page.menu" %}
        {% block name="sidebar" class="ProgramCms\\AdminBundle\\Block\\Sidebar" template="@ProgramCmsAdminBundle/sidebar.html.twig" %}{% endBlock %}
    {% endReferenceContainer %}
    {% referenceContainer name="main.top" %}
        {% container name="navbar.header" htmlTag="nav" htmlClass="navbar navbar-expand-lg content-header" %}
            {# Page Title Block #}
            {% block name="page.title" class="ProgramCms\\ThemeBundle\\Block\\Html\\Title" template="@ProgramCmsThemeBundle/title.html.twig" %}{% endBlock %}
            {# Admin Navbar Fast Access Container #}
            {% container name="navbar.collapse" htmlTag="div" htmlClass="collapse navbar-collapse justify-content-end" %}
                {% block name="account.dropdown" class="ProgramCms\\AdminBundle\\Block\\Account\\Navigation" template="@ProgramCmsAdminBundle/admin_account_dropdown.html.twig" %}{% endBlock %}
            {% endContainer %}
        {% endContainer %}
        {% container name="breadcrumb" htmlTag="div" htmlClass="breadcrumb-content" before="-" %}
            {% block name="breadcrumb.content" class="ProgramCms\\ThemeBundle\\Block\\Html\\Breadcrumb" template="@ProgramCmsThemeBundle/breadcrumb.html.twig" %}{% endBlock %}
        {% endContainer %}
    {% endReferenceContainer %}
    {% referenceContainer name="page.main.actions" %}
        {% container name="buttons.bar" htmlTag="div" htmlClass="container-fluid ef-buttons-bar" %}{% endContainer %}
    {% endReferenceContainer %}
    {% referenceContainer name="page.messages" %}
        {% block name="page.alerts.block" class="ProgramCms\\CoreBundle\\View\\Element\\Template" template="@ProgramCmsAdminBundle/flashes.html.twig" %}{% endBlock %}
    {% endReferenceContainer %}
    {% referenceContainer name="footer.container" %}
        {% container name="footer.content" htmlTag="div" htmlClass="footer-content row" %}
            {% container name="footer.legal.copyright" htmlTag="div" htmlClass="col-md-6" %}
                {% block name="copyright" class="ProgramCms\\AdminBundle\\Block\\Page\\Copyright" %}{% endBlock %}
            {% endContainer %}
            {% container name="footer.legal.system" htmlTag="div" htmlClass="col-md-6 text-end" %}
                {% block name="version" class="ProgramCms\\AdminBundle\\Block\\Page\\Footer" template="@ProgramCmsAdminBundle/footer.html.twig" %}{% endBlock %}
                {% block name="site.update" class="ProgramCms\\AdminBundle\\Block\\Legal\\SiteUpdate" template="@ProgramCmsAdminBundle/site_update.html.twig" %}{% endBlock %}
                {% block name="links" class="ProgramCms\\CoreBundle\\View\\Element\\Template" template="@ProgramCmsAdminBundle/links.html.twig" %}{% endBlock %}
            {% endContainer %}
        {% endContainer %}
    {% endReferenceContainer %}
{% endPage %}