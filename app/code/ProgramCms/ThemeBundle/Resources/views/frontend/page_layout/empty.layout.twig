{% layout %}
    {% container name="root" %}
        {% container name="head" htmlTag="head" %}{% endContainer %}
        {% container name="body" htmlTag="body" %}
            {% container name="before.body" %}{% endContainer %}
            {% container name="page.wrapper" htmlTag="div" htmlClass="page-wrapper" %}
                {% container name="global.notices" before="-" %}{% endContainer %}
                {% container name="main.content" htmlTag="main" htmlClass="page-main container" %}
                    {% container name="columns" htmlTag="div" htmlClass="row me-0 columns" %}
                        {% container name="main" htmlTag="div" htmlClass="col main-col" %}{% endContainer %}
                    {% endContainer %}
                {% endContainer %}
            {% endContainer %}
            {% container name="after.body" %}{% endContainer %}
        {% endContainer %}
    {% endContainer %}
{% endLayout %}