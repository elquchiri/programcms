{#
/*
 * Load Global app js file
 * Merge All static Javascript files
 * Ensure JS files being loaded after DOM ready
 */
#}
{% for jsFile in block.getStaticJs() %}
    <script type="text/javascript" src="{{ asset(jsFile) }}"></script>
{% endfor %}
<script type="text/javascript" src="{{ block.getAppJs() }}"></script>