{% EFPage layout="3columns" %}
    {% EFCss ['bootstrap.min.css', 'bundles/programcmstheme/css/main.css', 'bundles/programcmstheme/css/top-panel.css', 'bundles/programcmstheme/css/footer.css', 'bundles/programcmstheme/css/breadcrumb.css'] %}
    {% EFJs ['jquery.min.js', 'underscore-umd-min.js', 'popper.min.js', 'bootstrap.bundle.min.js'] %}

    {% EFReferenceContainer name="header.container" %}
        {% EFContainer name="header.panel.wrapper" htmlTag="div" htmlClass="panel wrapper" %}
            {% EFContainer name="header.panel" htmlTag="div" htmlClass="panel header container" %}
                {# Website switcher dropdown #}
                {% EFBlock name="website.switcher" class="ProgramCms\\WebsiteBundle\\Block\\WebsiteSwitcher" template="@ProgramCmsWebsiteBundle/website_switcher.html.twig" %}{% endEFBlock %}
                {# Account navigation links #}
                {% EFBlock name="website.navigation" class="ProgramCms\\UserBundle\\Block\\Account\\Navigation" template="@ProgramCmsUserBundle/account_navigation.html.twig" %}{% endEFBlock %}
            {% endEFContainer %}
        {% endEFContainer %}
        {% EFContainer name="header.wrapper" htmlTag="div" htmlClass="panel content" %}
            {% EFContainer name="navbar" htmlTag="nav" htmlClass="navbar navbar-expand-lg container" %}
                {# Website LOGO EFBlock #}
                {% EFBlock name="logo" class="ProgramCms\\ThemeBundle\\Block\\Html\\Header\\Logo" template="@ProgramCmsThemeBundle/logo.html.twig" %}{% endEFBlock %}
                {# Website Navbar Toggler EFBlock #}
                {% EFBlock name="navbar.togller" class="ProgramCms\\UserBundle\\Block\\Account\\Navigation" template="@ProgramCmsThemeBundle/navbar_toggler.html.twig" %}{% endEFBlock %}
                {# Website Navbar Fast Access Container #}
                {% EFContainer name="navbar.collapse" htmlTag="div" htmlClass="collapse navbar-collapse justify-content-end" %}{% endEFContainer %}
            {% endEFContainer %}
        {% endEFContainer %}
    {% endEFReferenceContainer %}

    {% EFReferenceContainer name="page.top" %}
        {# Navigation Section EFBlock #}
        {% EFBlock name="navigation.bar" class="ProgramCms\\UserBundle\\Block\\Account\\Navigation" template="@ProgramCmsThemeBundle/navigation.bar.html.twig" %}{% endEFBlock %}
        {% EFContainer name="top.container" htmlTag="div" htmlClass="top-container" %}{% endEFContainer %}
        {# Breadcrump EFBlock #}
        {% EFBlock name="breadcrumb" class="ProgramCms\\UserBundle\\Block\\Account\\Navigation" template="@ProgramCmsThemeBundle/breadcrumb.html.twig" %}{% endEFBlock %}
    {% endEFReferenceContainer %}

    {% EFReferenceContainer name="main" %}
        {% EFContainer name="content" %}{% endEFContainer %}
    {% endEFReferenceContainer %}

    {% EFReferenceContainer name="footer.container" %}
        {% EFContainer name="footer" htmlTag="div" htmlClass="footer content" %}
            {% EFBlock name="footer.content" class="ProgramCms\\WebsiteBundle\\Block\\WebsiteSwitcher" template="@ProgramCmsThemeBundle/footer.html.twig" %}{% endEFBlock %}
        {% endEFContainer %}
    {% endEFReferenceContainer %}
{% endEFPage %}