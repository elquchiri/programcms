{% page %}
    {% referenceBlock name="navigation.block" %}
        {% block name="navigation.posts.link" class="ProgramCms\\ThemeBundle\\Block\\Html\\Item" %}
            {% arguments %}
                {% argument name="label" type="string" %}Posts{% endArgument %}
                {% argument name="path" type="string" %}post_user_index{% endArgument %}
                {% argument name="sortOrder" type="string" %}2{% endArgument %}
                {% argument name="isAuth" type="boolean" %}true{% endArgument %}
            {% endArguments %}
        {% endBlock %}
        {% block name="navigation.trends.link" class="ProgramCms\\ThemeBundle\\Block\\Html\\Item" %}
            {% arguments %}
                {% argument name="label" type="string" %}Trends{% endArgument %}
                {% argument name="path" type="string" %}post_index_trend{% endArgument %}
                {% argument name="sortOrder" type="string" %}6{% endArgument %}
            {% endArguments %}
        {% endBlock %}
        {% block name="navigation.archive.link" class="ProgramCms\\ThemeBundle\\Block\\Html\\Item" %}
            {% arguments %}
                {% argument name="label" type="string" %}Archive{% endArgument %}
                {% argument name="path" type="string" %}post_index_archive{% endArgument %}
                {% argument name="sortOrder" type="string" %}7{% endArgument %}
            {% endArguments %}
        {% endBlock %}
    {% endReferenceBlock %}
{% endPage %}