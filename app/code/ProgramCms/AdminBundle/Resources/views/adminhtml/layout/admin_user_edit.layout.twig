{% page layout="admin-2columns-left" %}
    {% referenceContainer name="content" %}
        {% block name="admin_user_form" class="ui.block.form" %}
            {% arguments %}
                {% argument name="data" type="array" %}
                    {% argument name="label" type="string" %}Admin Users{% endArgument %}
                {% endArgument %}
                {% argument name="buttons" type="array" %}
                    {% argument name="back" type="string" %}ProgramCms\AdminBundle\Model\Provider\Button\AdminUser\BackButton{% endArgument %}
                    {% argument name="save" type="string" %}ProgramCms\AdminBundle\Model\Provider\Button\AdminUser\SaveButton{% endArgument %}
                {% endArgument %}
                {% argument name="dataSource" type="array" %}
                    {% argument name="dataProvider" type="array" %}
                        {% argument name="class" type="string" %}ProgramCms\AdminBundle\Model\Provider\Listing\AdminUser\DataProvider{% endArgument %}
                        {% argument name="primaryFieldName" type="string" %}user_id{% endArgument %}
                        {% argument name="requestFieldName" type="string" %}id{% endArgument %}
                    {% endArgument %}
                    {% argument name="settings" type="array" %}
                        {% argument name="submitUrl" type="string" %}admin_user_save{% endArgument %}
                    {% endArgument %}
                {% endArgument %}
                {% argument name="layout" type="array" %}
                    {% argument name="navContainerName" type="string" %}sidebar.main{% endArgument %}
                    {% argument name="type" type="string" %}tabs{% endArgument %}
                {% endArgument %}
            {% endArguments %}
            {% block name="user_view" class="ui.block.form.fieldset" %}
                {% arguments %}
                    {% argument name="data" type="array" %}
                        {% argument name="label" type="string" %}Account Interface{% endArgument %}
                    {% endArgument %}
                {% endArguments %}
                {% block name="user_id" class="ui.block.form.field" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="type" type="string" %}hidden{% endArgument %}
                            {% argument name="isRequired" type="boolean" %}true{% endArgument %}
                            {% argument name="source" type="string" %}admin_user_form{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="first_name" class="ui.block.form.field" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="type" type="string" %}text{% endArgument %}
                            {% argument name="label" type="string" %}First Name{% endArgument %}
                            {% argument name="isRequired" type="boolean" %}true{% endArgument %}
                            {% argument name="source" type="string" %}admin_user_form{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="last_name" class="ui.block.form.field" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="type" type="string" %}text{% endArgument %}
                            {% argument name="label" type="string" %}Last Name{% endArgument %}
                            {% argument name="isRequired" type="boolean" %}true{% endArgument %}
                            {% argument name="source" type="string" %}admin_user_form{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="email" class="ui.block.form.field" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="type" type="string" %}text{% endArgument %}
                            {% argument name="label" type="string" %}Email{% endArgument %}
                            {% argument name="isRequired" type="boolean" %}true{% endArgument %}
                            {% argument name="source" type="string" %}admin_user_form{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="new_password" class="ui.block.form.field" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="type" type="string" %}password{% endArgument %}
                            {% argument name="label" type="string" %}New Password{% endArgument %}
                            {% argument name="source" type="string" %}admin_user_form{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="password_confirmation" class="ui.block.form.field" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="type" type="string" %}password{% endArgument %}
                            {% argument name="label" type="string" %}Password Confirmation{% endArgument %}
                            {% argument name="source" type="string" %}admin_user_form{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="interface_locale" class="ui.block.form.field" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="type" type="string" %}select{% endArgument %}
                            {% argument name="label" type="string" %}Interface Locale{% endArgument %}
                            {% argument name="sourceModel" type="string" %}ProgramCms\UiBundle\Model\Provider\DataSource\Locales{% endArgument %}
                            {% argument name="source" type="string" %}admin_user_form{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="roles" class="ui.block.form.field" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="type" type="string" %}multiselect{% endArgument %}
                            {% argument name="label" type="string" %}Roles{% endArgument %}
                            {% argument name="sourceModel" type="string" %}ProgramCms\AclBundle\Model\Provider\DataSource\Acl\Roles{% endArgument %}
                            {% argument name="source" type="string" %}admin_user_form{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
            {% endBlock %}
            {% block name="fieldset3" class="ui.block.form.fieldset" %}
                {% arguments %}
                    {% argument name="data" type="array" %}
                        {% argument name="label" type="string" %}User Identity Verification{% endArgument %}
                        {% argument name="open" type="boolean" %}true{% endArgument %}
                        {% argument name="collapse" type="boolean" %}true{% endArgument %}
                    {% endArgument %}
                {% endArguments %}

                {% block name="password" class="ui.block.form.field" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="type" type="string" %}password{% endArgument %}
                            {% argument name="label" type="string" %}Your Password{% endArgument %}
                            {% argument name="isRequired" type="boolean" %}true{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
            {% endBlock %}
        {% endBlock %}
    {% endReferenceContainer %}
{% endPage %}