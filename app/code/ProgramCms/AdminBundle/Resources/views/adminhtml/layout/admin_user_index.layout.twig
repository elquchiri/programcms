{% page %}
    {% referenceContainer name="content" %}
        {% block name="admin_users_listing" class="ui.block.listing" %}
            {% arguments %}
                {% argument name="buttons" type="array" %}
                    {% argument name="new" type="string" %}ProgramCms\AdminBundle\Model\Provider\Button\AdminUser\NewButton{% endArgument %}
                {% endArgument %}
                {% argument name="dataSource" type="array" %}
                    {% argument name="dataProvider" type="array" %}
                        {% argument name="class" type="string" %}ProgramCms\AdminBundle\Model\Provider\Listing\AdminUser\DataProvider{% endArgument %}
                        {% argument name="primaryFieldName" type="string" %}user_id{% endArgument %}
                    {% endArgument %}
                {% endArgument %}
            {% endArguments %}
            {% block name="toolbar" class="ui.block.listing.toolbar" %}
                {% block name="filters" class="ui.block.listing.filters" %}{% endBlock %}
                {% block name="search" class="ui.block.listing.search" %}{% endBlock %}
                {% block name="massActions" class="ui.block.listing.massActions" %}
                    {% arguments %}
                        {% argument name="settings" type="array" %}
                            {% argument name="delete" type="array" %}
                                {% argument name="url" type="string" %}delete{% endArgument %}
                                {% argument name="label" type="string" %}Delete All{% endArgument %}
                            {% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="pagination" class="ui.block.listing.pagination" %}{% endBlock %}
            {% endBlock %}
            {% block name="columns" class="ui.block.columns" %}
                {% block name="selectionsColumn" class="ui.block.selectionsColumn" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="source" type="string" %}admin_users_listing{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="user_id" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}#{% endArgument %}
                            {% argument name="source" type="string" %}admin_users_listing{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="first_name" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}First Name{% endArgument %}
                            {% argument name="source" type="string" %}admin_users_listing{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="last_name" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}Last Name{% endArgument %}
                            {% argument name="source" type="string" %}admin_users_listing{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="email" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}Email{% endArgument %}
                            {% argument name="source" type="string" %}admin_users_listing{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="interface_locale" class="ui.block.column" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}Locale{% endArgument %}
                            {% argument name="source" type="string" %}admin_users_listing{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
                {% block name="actionsColumn" class="ProgramCms\\AdminBundle\\Ui\\Grid\\Column\\UserAdmin\\Actions" %}
                    {% arguments %}
                        {% argument name="data" type="array" %}
                            {% argument name="label" type="string" %}Actions{% endArgument %}
                            {% argument name="source" type="string" %}admin_users_listing{% endArgument %}
                        {% endArgument %}
                    {% endArguments %}
                {% endBlock %}
            {% endBlock %}
        {% endBlock %}
    {% endReferenceContainer %}
{% endPage %}