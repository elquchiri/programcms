{% extends '@ElectroForumsTheme/base.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <link rel="stylesheet" href="{{ asset('bundles/electroforumstheme/css/frontend/main.css') }}"/>
    <link rel="stylesheet" href="{{ asset('bundles/electroforumstheme/css/frontend/top.panel.css') }}"/>
    <link rel="stylesheet" href="{{ asset('bundles/electroforumstheme/css/frontend/header.css') }}"/>
    <link rel="stylesheet" href="{{ asset('bundles/electroforumstheme/css/frontend/breadcrumb.css') }}"/>
    <link rel="stylesheet" href="{{ asset('bundles/electroforumstheme/css/frontend/footer.css') }}"/>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript" src="{{ asset('bundles/electroforumstheme/js/lib/jquery.min.js') }}"></script>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.6/dist/umd/popper.min.js" integrity="sha384-oBqDVmMz9ATKxIep9tiCxS/Z9fNfEXiDAYTujMAeBAsjFuCZSmKbSSUnQlmh/jp3" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.min.js" integrity="sha384-mQ93GR66B00ZXjt0YO5KlohRA5SY2XofN4zfuZxLkoj1gXtW8ANNCe9d5Y3eG5eD" crossorigin="anonymous"></script>
    <script>
        const tooltipTriggerList = document.querySelectorAll('[data-bs-toggle="tooltip"]');
        const tooltipList = [...tooltipTriggerList].map(tooltipTriggerEl => new bootstrap.Tooltip(tooltipTriggerEl));
    </script>
{% endblock %}

{% block body %}
    {% include '@ElectroForumsTheme/frontend/top.panel.html.twig' %}

    {% include '@ElectroForumsTheme/frontend/header.html.twig' %}

    {% include '@ElectroForumsTheme/frontend/navigation.bar.html.twig' %}

    {% include '@ElectroForumsTheme/frontend/breadcrumb.html.twig' %}

    <div class="content">
        {% block content %}{% endblock %}
    </div>

    {% include '@ElectroForumsTheme/frontend/footer.html.twig' %}

{% endblock %}